{
  "meta": {
    "options": {
      "linkedFile": "/views/admin/DbRoutines/manual_encrypt.ejs",
      "linkedForm": "form1"
    },
    "$_GET": [
      {
        "type": "text",
        "name": "unencrypttext"
      }
    ],
    "$_POST": [
      {
        "type": "text",
        "fieldName": "encText",
        "multiple": true,
        "name": "encText"
      },
      {
        "type": "text",
        "fieldName": "key",
        "name": "key"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "encString",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_GET.unencrypttext.encrypt($_POST.key)}}"
        },
        "meta": [],
        "outputType": "text",
        "output": true
      },
      {
        "name": "wait",
        "module": "core",
        "action": "wait",
        "options": {
          "delay": 100
        },
        "output": false
      }
    ]
  }
}