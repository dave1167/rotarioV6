                                        <input type="password" id="inp_password" name="password" class="form-control ms-2" placeholder="Password">
                                    </div>
                                </div>
                                <div class="row mt-4">
                                    <div class="col">
                                        <button id="login1_submit" class="btn btn-success mt-2 mb-2 ms-2" type="submit">Submit</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
            </div>
        </div>
    </div>
    <div class="col"></div>

</div>

</div>