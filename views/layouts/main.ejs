          <a class="btn btn-primary ms-2" href="/login">Login</a>
        </div>
      </div>
    </div>
  </nav>

  <main is="dmx-view" id="content">
    <%- await include(content, locals); %>
  </main>
  <script src="/bootstrap/5/js/bootstrap.bundle.min.js"></script>
